   "source": [
    "def clean_text(text):\n",
    "    text = re.sub(r'\\n', ' ', text)\n",
    "    text = re.sub('- ', '', text)\n",
    "    text = re.sub(r'[0-9]', '', text)\n",
    "    return text"
   ]
  },
  {
